<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_marginStart="20dp">

<TextView
    android:id="@+id/home_text_8"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="@string/home_text_8"
    android:textColor="@color/colorBlack"
    android:textSize="20sp"
    android:textStyle="bold"
    android:fontFamily="@font/jalnan"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent" />

<TextView
    android:id="@+id/home_text_category_3"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:textColor="@color/colorBlack"
    tools:text="카페"
    android:textSize="20sp"
    android:textStyle="bold"
    android:fontFamily="@font/jalnan"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintStart_toEndOf="@id/home_text_8"
    />

<TextView
    android:id="@+id/home_text_9"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:textColor="@color/colorBlack"
    android:text="@string/home_text_9"
    android:textSize="20sp"
    android:textStyle="bold"
    android:fontFamily="@font/jalnan"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintStart_toEndOf="@id/home_text_category_3"
    />
</androidx.constraintlayout.widget.ConstraintLayout>